    E->QR_AnZtSZnAt_nrm = (double *) wkspace_all0c(E->A_nrows*D->T_ncols*nbins*sizeof(double)); if (!E->QR_AnZtSZnAt_nrm){ printf(" %% Warning! not enough memory in bcc_init_X_QX\n");}
